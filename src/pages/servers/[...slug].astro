---
import "../../components/react/servers/style.css";
import BaseLayout from "../../layouts/BaseLayout.astro";
import ServerStatus from "../../components/react/servers/ServerStatus.jsx";

export function getStaticPaths () {
 return [
    {params: {slug: undefined}}
  ];
}
const { slug } = Astro.params;
---

<BaseLayout title="Intranet" sideBarActiveItemID="projects">
