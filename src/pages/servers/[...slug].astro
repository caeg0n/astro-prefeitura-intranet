    <div class="text-3xl w-full font-bold mb-5">STATUS DOS SERVIDORES</div>
    <div class="mt-12"> 
      <ServerStatus client:load websocketUrl="ws://192.168.1.15:1027" />
    </div>
  </div>
</BaseLayout>
